{
    "compilerOptions": {
      "lib": ["ES6","dom"],
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "strict": true,
      "resolveJsonModule": true,
      "skipLibCheck": true
    }
  }